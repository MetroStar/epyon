{
  "name": "advana-marketplace-docker-commands",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "docker:run": "docker run -p 8080:8080 advana-marketplace",
    "docker:build": "docker-compose up --build",
    "docker:prune": "docker system prune -af",
    "docker:stop": "docker-compose stop",
    "docker:kill": "docker-compose kill",
    "sonar": "./run-sonar-analysis.sh",
    "sonar:analyze": "npx sonarqube-scanner",
    "security:scan": "./run-trufflehog-scan.sh",
    "trufflehog": "./run-trufflehog-scan.sh",
    "trufflehog:filesystem": "./run-trufflehog-scan.sh filesystem",
    "trufflehog:images": "./run-trufflehog-scan.sh images",
    "trufflehog:containers": "./run-trufflehog-scan.sh containers",
    "secret:scan": "./run-trufflehog-scan.sh",
    "secret:filesystem": "./run-trufflehog-scan.sh filesystem",
    "secret:containers": "./run-trufflehog-scan.sh images",
    "secret:analyze": "./analyze-trufflehog-results.sh",
    "virus:scan": "./run-clamav-scan.sh",
    "virus:analyze": "./analyze-clamav-results.sh",
    "clamav": "./run-clamav-scan.sh",
    "antivirus": "./run-clamav-scan.sh",
    "helm:build": "./run-helm-build.sh",
    "helm:analyze": "./analyze-helm-results.sh",
    "helm:lint": "helm lint ./chart",
    "helm:package": "helm package ./chart --destination ./helm-packages",
    "helm:template": "helm template advana-marketplace ./chart",
    "checkov:scan": "./run-checkov-scan.sh",
    "checkov:helm": "./run-checkov-scan.sh",
    "iac:scan": "./run-checkov-scan.sh",
    "checkov:analyze": "./analyze-checkov-results.sh",
    "iac:analyze": "./analyze-checkov-results.sh",
    "trivy:scan": "./run-trivy-scan.sh",
    "trivy:analyze": "./analyze-trivy-results.sh",
    "trivy:filesystem": "./run-trivy-scan.sh filesystem",
    "trivy:images": "./run-trivy-scan.sh images",
    "trivy:base": "./run-trivy-scan.sh base",
    "trivy:registry": "./run-trivy-scan.sh registry",
    "trivy:kubernetes": "./run-trivy-scan.sh kubernetes",
    "grype:scan": "./run-grype-scan.sh",
    "grype:analyze": "./analyze-grype-results.sh",
    "grype:filesystem": "./run-grype-scan.sh filesystem",
    "grype:images": "./run-grype-scan.sh images",
    "grype:base": "./run-grype-scan.sh base",
    "grype:registry": "./run-grype-scan.sh registry",
    "xeol:scan": "./run-xeol-scan.sh",
    "xeol:analyze": "./analyze-xeol-results.sh",
    "xeol:filesystem": "./run-xeol-scan.sh filesystem",
    "xeol:images": "./run-xeol-scan.sh images",
    "xeol:registry": "./run-xeol-scan.sh registry",
    "eol:scan": "./run-xeol-scan.sh",
    "eol:analyze": "./analyze-xeol-results.sh",
    "eol:filesystem": "./run-xeol-scan.sh filesystem",
    "eol:containers": "./run-xeol-scan.sh images",
    "vulnerability:scan": "./run-trivy-scan.sh && ./run-grype-scan.sh",
    "container:scan": "./run-trivy-scan.sh && ./run-grype-scan.sh",
    "image:scan": "./run-grype-scan.sh",
    "sbom:generate": "./run-grype-scan.sh",
    "security:analyze": "./analyze-trufflehog-results.sh && ./analyze-checkov-results.sh && ./analyze-trivy-results.sh && ./analyze-grype-results.sh && ./analyze-xeol-results.sh",
    "security:consolidate": "./consolidate-security-reports.sh",
    "reports:consolidate": "./consolidate-security-reports.sh",
    "reports:dashboard": "open ./security-reports/dashboards/security-dashboard.html",
    "dashboard": "open ./security-reports/dashboards/security-dashboard.html",
    "target:scan": "./scripts/run-target-security-scan.sh",
    "target:quick": "./scripts/run-target-security-scan.sh \"$npm_config_target\" quick",
    "target:full": "./scripts/run-target-security-scan.sh \"$npm_config_target\" full",
    "target:images": "./scripts/run-target-security-scan.sh \"$npm_config_target\" images",
    "target:analysis": "./scripts/run-target-security-scan.sh \"$npm_config_target\" analysis",
    "aws:ecr:auth": "./scripts/aws-ecr-helm-auth.sh",
    "helm:deps:resolve": "./scripts/resolve-helm-dependencies.sh",
    "helm:deps:stub": "./scripts/create-stub-dependencies.sh"
  },
  "devDependencies": {
    "sonarqube-scanner": "^4.3.2"
  }
}
