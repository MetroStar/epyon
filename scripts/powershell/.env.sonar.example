# SonarQube Configuration Example
# Copy this file to .env.sonar and fill in your values

# Your SonarQube server URL
export SONAR_HOST_URL='https://sonarqube.example.com'

# Your SonarQube authentication token
export SONAR_TOKEN='your-sonarqube-token-here'

# Your project key/name (will be used as the identifier in SonarQube)
export SONAR_PROJECT_KEY='my-project-name'

# Optional: Override the sources directory
# export SONAR_SOURCES='./src'
